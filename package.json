{
  "name": "eds-self-service-website",
  "private": true,
  "version": "1.0.0",
  "description": "Fast Sites",
  "scripts": {
    "dev": "aem up --no-open --url https://main--eds-self-service-website--headwire-edge-delivery.aem.live",
    "lint:js": "eslint .",
    "lint:js-fix": "eslint . --fix",
    "lint:css": "stylelint blocks/**/*.css styles/*.css",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:fix": "npm run lint:js-fix && npm run lint:css",
    "prettier": "npx prettier --write ."
  },
  "devDependencies": {
    "@adobe/aem-cli": "^16.10.10",
    "@babel/core": "7.26.10",
    "@babel/eslint-parser": "7.26.10",
    "@eslint/js": "^9.22.0",
    "chai": "5.2.0",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "globals": "^16.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.5.0",
    "stylelint": "16.16.0",
    "stylelint-config-standard": "37.0.0"
  },
  "lint-staged": {
    "**/*.{js,mjs,cjs}": [
      "prettier --write",
      "eslint ."
    ],
    "*.js": "eslint --cache --fix",
    "*.css": "stylelint --fix"
  }
}
